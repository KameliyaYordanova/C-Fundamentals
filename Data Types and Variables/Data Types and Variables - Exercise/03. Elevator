using System;

namespace DataType
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int capacity = int.Parse(Console.ReadLine());
            int courses = 0;

            for (int i = 0; i < n; i++)
            {
                courses = (int)Math.Ceiling((double)n / capacity);
            }

            Console.WriteLine(courses);
        }
    }
}
